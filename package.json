{
  "name": "api-boilerplate",
  "version": "1.1.0",
  "description": "A boilerplate for REST API development with Node.js, ExpressJS and MongoDB.",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "dev": "nodemon src/index.js --exec babel-node",
    "start": "npm run build && nodemon dist/index.js --exec babel-node"
  },
  "engines": {
    "node": ">=8.17.0"
  },
  "keywords": [
    "boilerplate",
    "starter",
    "nodejs",
    "api",
    "REST",
    "express",
    "mongodb"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ringate/nodejs-microservice-boilerplate.git"
  },
  "homepage": "https://github.com/ringate/nodejs-microservice-boilerplate#readme",
  "author": "Adrian Li <ringate@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.31",
    "mongodb": "^3.5.9",
    "morgan": "^1.10.0",
    "rotating-file-stream": "^2.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "nodemon": "^2.0.19"
  }
}
